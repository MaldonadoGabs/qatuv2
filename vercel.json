{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/src/styles/:path*", "destination": "/src/styles/:path*" },
    { "source": "/src/pages/:path*", "destination": "/src/pages/:path*" },
    { "source": "/public/:path*", "destination": "/public/:path*" },
    { "source": "/dist/:path*", "destination": "/dist/:path*" }
  ]
}
